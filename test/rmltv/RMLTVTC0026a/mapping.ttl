@prefix rr: <http://www.w3.org/ns/r2rml#> .
@prefix ex: <http://example.com/> .
@prefix rml: <http://semweb.mmlab.be/ns/rml#> .

<TriplesMap1>
    a rr:TriplesMap;
 	rml:logicalSource [
                     rml:query """
                       SELECT ID, UNNEST(string_split(COL[2:-1], ',')) AS COL
                       FROM 'test/rmltv/RMLTVTC0026a/mixed_content.csv'
                       """
                       ];

	rr:subjectMap [ rml:reference "ID"; rr:termType rr:BlankNode; ];
	rr:predicateObjectMap
    [
      rr:predicate		ex:col;
      rr:objectMap		[ rml:reference "COL" ];
    ];
.
